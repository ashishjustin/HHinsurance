package org.brms.hhinsurance;
global java.util.List construction;
global java.util.List constructionType;
global java.util.List masonryConstruction;
global java.util.List frameConstruction;
global java.util.List hardiplank;

rule "Structure Type : NonCondominium"
	when 
	    ConstructionMapping(structureType not in ("Condominium"))
	then
	    drools.setFocus("NonCondominiumStories");
end

rule "Structure Type : Condominium"
	when 
		ConstructionMapping(structureType in ("Condominium"))
	then
	    drools.setFocus("CondominiumStories");
end


rule "number of Stories Non Condo : 1"
ruleflow-group "NonCondominiumStories"
	when
		ConstructionMapping(stories == 1)
    then
        drools.setFocus("StoriesNonCondo1");
end

rule "number of Stories Non Condo : 2"
ruleflow-group "NonCondominiumStories"
	when
		ConstructionMapping(stories == 2)
    then
        drools.setFocus("StoriesNonCondo2");
end

rule "number of Stories NonCondo greater than 2"
ruleflow-group "NonCondominiumStories"
	when
		ConstructionMapping(stories > 2)
    then
        drools.setFocus("StoriesNonCondo3");
end

rule "1: NonCondominium : Construction material  : Stucco"
ruleflow-group "StoriesNonCondo1"
	when
		ConstructionMapping(constructionMaterial.equalsIgnoreCase("stucco"))
    then
    	drools.setFocus("ConstructionStuccoNonCondo1");
end

rule "1: NonCondominium : Stucco : 100% Masonry"
ruleflow-group "ConstructionStuccoNonCondo1"
	when
	    ConstructionMapping(constructionOption.equalsIgnoreCase("100% Masonry"))
	then
