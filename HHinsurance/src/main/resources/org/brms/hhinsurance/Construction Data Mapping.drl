	when
	    ConstructionMapping(constructionOption.equalsIgnoreCase("100% Concrete Block"))
	then
	    construction.add("Concrete Block");
	    constructionType.add("Masonry");
	    masonryConstruction.add("Concrete Block");
	    frameConstruction.add("");
	    hardiplank.add("0");
end

rule "2: Concrete Block Painted : 100% ConcreteBlock : Flat Roof"
ruleflow-group "ConstructionConcreteBlockPaintedCondo2"
	when
	    ConstructionMapping(constructionOption.equalsIgnoreCase("100% Concrete Block") && roofShape.equalsIgnoreCase("flat"))
	then
	    construction.clear();
	    constructionType.clear();
	    masonryConstruction.clear();
	    frameConstruction.clear();
	    hardiplank.clear();
	    construction.add("Superior");
	    constructionType.add("Masonry");
	    masonryConstruction.add("Superior");
	    frameConstruction.add("");
	    hardiplank.add("0");
end


